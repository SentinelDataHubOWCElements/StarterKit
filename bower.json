    "owc-core-utils":"SentinelDataHubOWCElements/owc-core-utils#master",
    "owc-hello":"SentinelDataHubOWCElements/owc-hello#master",
    "owc-core-main-menu":"SentinelDataHubOWCElements/owc-core-main-menu#master",

